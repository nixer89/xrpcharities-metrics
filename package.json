{
  "name": "xrpcharities-metrics",
  "version": "2.0.0",
  "description": "Metrics page for the XRP Charities",
  "main": "main.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --prod",
    "upload": "ng build --prod & aws s3 --profile myevride cp ./dist/xrpcharities-metrics s3://charities.xrptipbot-stats.com/ --recursive --include '*' --acl public-read --cache-control public,max-age=86400,no-transform",
    "invalidate": "aws cloudfront --profile myevride create-invalidation --distribution-id E1QC3QL9N37K2N --paths /* & aws cloudfront --profile myevride create-invalidation --distribution-id E13SE86D3BY0CS --paths /*",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nixer89/xrpcharities-metrics.git"
  },
  "keywords": [
    "XRP",
    "Charities",
    "Metrics",
    "XRPTipbot"
  ],
  "author": "Good Souls Group",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nixer89/xrpcharities-metrics/issues"
  },
  "homepage": "https://github.com/nixer89/xrpcharities-metrics#readme",
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "chart.js": "^2.9.3",
    "core-js": "^2.6.10",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^10.17.5",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
